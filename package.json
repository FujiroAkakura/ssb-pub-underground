{
  "name": "ssb-pub-underground",
  "version": "0.1.0",
  "description": "Host your own Secure ScuttleButt (SSB) pub in a docker container at home",
  "scripts": {
    "build": "docker build -t FujiroAkakura/ssb-pub-underground:$(jq -r .version package.json) .",
